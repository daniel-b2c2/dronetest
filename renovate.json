{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^\\.drone\\.yml"],
      "matchStrings": ["^ *image: &bazel_image.*:(?<currentValue>.*?)\\n"],
      "depNameTemplate": "busybox",
      "datasourceTemplate": "docker"
    }
  ],
  "dependencyDashboard": true,
  "extends": [
    "config:recommended"
  ]
}
